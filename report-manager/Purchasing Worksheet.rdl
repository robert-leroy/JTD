ApSupplier.Supplier, 
ApSupplier.SupplierName, 
InvWarehouse.UnitCost AS UNIT_COST, 
IsNull(CONVERT(char(12), qryPurchOrders.MLatestDueDate, 101),'No Order') AS LATEST_DUE_DATE,
--IsNull(CAST(qryPurchOrders.MLatestDueDate AS VARCHAR),'No Order') AS LATEST_DUE_DATE, 
InvWarehouse.SalesQty1 AS P1, 
InvWarehouse.SalesQty2 AS P2, 
InvWarehouse.SalesQty3 AS P3, 
InvWarehouse.SalesQty4 AS P4, 
InvWarehouse.SalesQty5 AS P5, 
InvWarehouse.SalesQty6 AS P6, 
InvWarehouse.SalesQty7 AS P7, 
InvWarehouse.SalesQty8 AS P8, 
InvWarehouse.SalesQty9 AS P9, 
InvWarehouse.SalesQty10 AS P10, 
InvWarehouse.SalesQty11 AS P11, 
InvWarehouse.SalesQty12 AS P12, 
InvWarehouse.SalesQty1 + InvWarehouse.SalesQty2 + InvWarehouse.SalesQty3 + InvWarehouse.SalesQty4 + InvWarehouse.SalesQty5 + InvWarehouse.SalesQty6 AS MTH6, 
InvWarehouse.SalesQty1 + InvWarehouse.SalesQty2 + InvWarehouse.SalesQty3 AS MTH3, 
(InvWarehouse.SalesQty1 + InvWarehouse.SalesQty2 + InvWarehouse.SalesQty3 + InvWarehouse.SalesQty4 + InvWarehouse.SalesQty5 + InvWarehouse.SalesQty6)/6 AS AVG6, 
[QtyOnHand]+[QtyOnOrder]-[QtyAllocated] AS FREE, 
(InvWarehouse.SalesQty1 + InvWarehouse.SalesQty2 + InvWarehouse.SalesQty3 )/3 AS AVG3,
(SELECT Sum([MOrderQty]-[MReceivedQty]) AS x
 FROM PorMasterDetail 
	INNER JOIN PorMasterHdr ON PorMasterDetail.PurchaseOrder = PorMasterHdr.PurchaseOrder
WHERE PorMasterDetail.MWarehouse = '01' AND PorMasterHdr.OrderStatus&lt;&gt;'*' AND PorMasterDetail.MStockCode = InvMaster.StockCode
) as POTOT,
qryPurchOrders.POAMT
FROM ((InvWarehouse 
INNER JOIN InvMaster ON InvWarehouse.StockCode = InvMaster.StockCode) 
INNER JOIN ApSupplier ON InvMaster.Supplier = ApSupplier.Supplier) 
LEFT OUTER JOIN 
(SELECT PorMasterDetail.MStockCode, 
	PorMasterDetail.MLatestDueDate, 
	Sum([MOrderQty]-[MReceivedQty]) AS POAMT
 FROM PorMasterDetail 
	INNER JOIN PorMasterHdr ON PorMasterDetail.PurchaseOrder = PorMasterHdr.PurchaseOrder
GROUP BY PorMasterDetail.MStockCode, PorMasterDetail.MLatestDueDate, PorMasterDetail.MWarehouse, PorMasterHdr.OrderStatus
HAVING (((Sum([MOrderQty]-[MReceivedQty]))&gt;0) AND ((PorMasterDetail.MWarehouse)='01') AND ((PorMasterHdr.OrderStatus)&lt;&gt;'*'))) qryPurchOrders ON InvWarehouse.StockCode = qryPurchOrders.MStockCode
WHERE (((InvMaster.SupercessionDate) Is Null) AND ((InvWarehouse.Warehouse)='01') AND ApSupplier.Supplier=@ParamSupplier AND (InvMaster.ProductClass IN (@ParamClass) or '-1' IN (@ParamClass)))
AND (@ParamShowZero = 1 OR (@ParamShowZero = 0 AND (QtyOnHand &gt; 0 OR QtyOnOrder&gt; 0 OR QtyAllocated&gt; 0 OR POAMT&gt;0)))
ORDER BY  STOCK_CODE</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ParamSupplier">
            <Value>=Parameters!ParamSupplier.Value</Value>
          </QueryParameter>
