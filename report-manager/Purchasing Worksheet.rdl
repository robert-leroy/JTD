            <Value>=Parameters!ParamClass.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ParamShowZero">
            <Value>=Parameters!ParamShowZero.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>Inventory</DataSourceName>
      </Query>
