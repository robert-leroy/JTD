  <Code>Function Median(p1, p2, p3, p4, p5, p6, p7, p8, p9, p10, p11, p12) As Integer

          Dim MonthQty(12) As Integer 

          MonthQty(0) = p1
          MonthQty(1) = p2
          MonthQty(2) = p3
          MonthQty(3) = p4
          MonthQty(4) = p5
          MonthQty(5) = p6
          MonthQty(6) = p7
          MonthQty(7) = p8
          MonthQty(8) = p9
          MonthQty(9) = p10
          MonthQty(10) = p11
          MonthQty(11) = p12

          Array.Sort(MonthQty)

          Median = (MonthQty(5) + MonthQty(6)) / 2

End Function

Function SuggestQty(SalesOrders As Integer, Free  As Integer, MonthsUse6  As Integer, MonthsUse3 As Integer) As Integer
    Dim TodayFactor As Integer
    Dim NeedWindow As Integer
    
    ' Calculate the percentage of the month remaining
    TodayFactor = Day(Now) / 30
    SuggestQty = TodayFactor

    NeedWindow = 2
    If (MonthsUse6 &gt; MonthsUse3) Then
        ' Need window (time between order and shipped) is determined by
        ' the 2.  We are currently using a 30 day window
        ' So a 60 day window would be 1.  An immded'ate window is 0
        SuggestQty = SalesOrders + (MonthsUse6 * NeedWindow) + (MonthsUse6 * TodayFactor) - Free
    Else
        SuggestQty = SalesOrders + (MonthsUse3 * NeedWindow) + (MonthsUse3 * TodayFactor) - Free
    End If
    
End Function</Code>
